<!-- views/profile.ejs -->
<!doctype html>
<html ng-app="myApp">
<head>
<title>msg</title>
<link rel="stylesheet" href="//netdna.bootstrapcdn.com/bootstrap/3.0.2/css/bootstrap.min.css">
<link rel="stylesheet" href="//netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.min.css">
<link rel="stylesheet" href="/stylesheets/style.css">
<script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.5.6/angular.min.js"></script>
<script src="https://ajax.googleapis.com/ajax/libs/angularjs/X.Y.Z/angular-animate.js"></script>
 <script type="text/javascript" src="javascripts/sendmsg.js" ></script>
<style>
	body 		{ padding-top:80px; word-wrap:break-word; }
</style>
</head>
<body>


<div class="container" ng-controller="myController">
<div class="row profile">

	<div class="col-md-3">
		<div class="profile-sidebar">
			<!-- SIDEBAR USERPIC -->
			<div class="profile-userpic">
				<img src="<%= picture %>" class="img-responsive" alt="">
			</div>
			<!-- END SIDEBAR USERPIC -->
			<!-- SIDEBAR USER TITLE -->
			<div class="profile-usertitle">
				<div class="profile-usertitle-name">
					<%= user.facebook.name  %>
				</div>
				<div class="profile-usertitle-job">
					<%= user.facebook.email || user.local.email%><br>
				</div>
			</div>
			<!-- END SIDEBAR USER TITLE -->
			<!-- SIDEBAR BUTTONS -->
			<div class="profile-userbuttons">
				<button type="button" class="btn btn-success btn-sm">Follow</button>
				<button type="button" class="btn btn-danger btn-sm">Message</button>
			</div>
			<!-- END SIDEBAR BUTTONS -->
			<!-- SIDEBAR MENU -->
			<div class="profile-usermenu">
				<ul class="nav">
					<li class="active">
						<a href="#">
						<i class="glyphicon glyphicon-home"></i>
						Overview </a>
					</li>
					<li>
						<a href="#">
						<i class="glyphicon glyphicon-user"></i>
						Account Settings </a>
					</li>
					<li>
						<a href="#" target="_blank">
						<i class="glyphicon glyphicon-ok"></i>
						Tasks </a>
					</li>
					<li>
						<a href="#">
						<i class="glyphicon glyphicon-flag"></i>
						Help </a>
					</li>
				</ul>
					
	<a href="/logout" class="btn btn-danger btn-sm logout" >Logout</a>
			</div>
			<!-- END MENU -->
		</div>
	</div>


<div class="col-md-9">
 <div class="profile-content">
 <div class="buttons col-md-6 col-md-offset-4">
<button type="button" ng-click="open()" ng-hide="buttons" class="btn btn-success btn-sm trydemo">Try demo</button>
<button ng-click="demo=false"  ng-hide="buttons" class="btn btn-success btn-sm trydemo">BACK!</button>
</div>
<div class="main-content col-md-6 col-md-offset-3" >

<form class="form-horizontal span5" ng-show="demo">
<h4>'How satisfied are you with the quality of education in your area?'<button type="button" class="close"  data-dismiss="modal" aria-label="Close"><span ng-click="openeditform()" class="glyphicon glyphicon-pencil" aria-hidden="true"></span></button> </h4>


        <fieldset>
          <legend>Enter Phone Number Give it a try</legend>
          
           <div class="input-group">
      <span class="input-group-btn">
        <button class="btn btn-default" type="button">+1</button>
      </span>
      <input type="text" class="form-control" ng-model="formData.num" placeholder="Enter Number">
    </div>
              <div class="clearfix">&nbsp;</div>
<button type="submit" ng-click="SendData()" class="btn btn-primary trydemo">Ask Feedback</button>
<a href="" ng-click="ReceiveData()" ng-model="show" class="btn btn-primary trydemo">Previous Records</a>
<h3 ng-show="formData.search" >{{success}}</h3>
          	</div><!--/form group-->
          
        </fieldset>
      </form>
  <br><br>




<div id ="table" class="col-md-8 col-md-offset-2" style = "margin-top:20px" ng-show="show">
<form class="form-inline" >
<div class="form-group">

<input type="text" class="form-control" required placeholder="Search" aria-describedby="sizing-addon3"  ng-model="search">
</div>
</form>
       <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span ng-click="close()" aria-hidden="true">&times;</span></button>

<table class= "table table-hover" >
				<thead>
					<tr>
						<th>Mobile.No</th>
						<th>Keyword</th>
						<th>User Response</th>
						
					</tr>
					<tbody>
						<tr ng-repeat="record in data|filter:search">
							<td>{{record.number}}</td>
							<td>{{record.zip}}</td>
							<td>{{record.response}}</td>
							
						</tr>

					</tbody>	
				</thead>
			</table>
		</div>
       <form class="form-horizontal span5 col-md-6 col-md-offset-3" ng-show="editform">
       <h3 ng-show="formData">{{success}}</h3>
<div class="input-group">
<textarea style="border-radius: 12px; padding:10px;" rows='8' cols="50" placeholder='YOUR QUESTION HERE?' required ng-model="formData.que"></textarea>
</div>
<br>
         
           <div class="input-group">
      <span class="input-group-btn">
        <button class="btn btn-default" type="button">+1</button>
      </span>
      <input type="text" class="form-control" ng-model="formData.num" placeholder="Enter Number">
    </div><br>
      <div class="input-group">
      <input type="text" class="form-control" ng-model="formData.keyword" placeholder="Enter Keyword">
    </div><br>
    <span>Response Type</span>
     <div class="form-check">
      <label class="form-check-label">
        <input type="radio" class="form-check-input" name="optionsRadios" ng-model="formData.radio" id="optionsRadios1" value="option1" checked>
        A.Excellent B.Very good C.Satisfactory D.Poor
      </label>
    </div>
    <div class="form-check">
    <label class="form-check-label">
        <input type="radio" class="form-check-input" name="optionsRadios" id="optionsRadios2" ng-model="formData.radio" value="option2">
        Rate between 1 to 5.
      </label>
    </div>
              <div class="clearfix">&nbsp;</div>
<button type="submit" ng-click="SendData()" class="btn btn-primary trydemo">Ask Feedback</button>
<a href="" ng-click="ReceiveData()" ng-model="show" class="btn btn-primary trydemo">Previous Records</a>

          	</div><!--/form group-->
          
 
      </form>
  <br><br>

        </div>
	</div>
</div>
</div>

<center>
<strong>Developed By <a href="#" target="_blank">Pratik.M</a></strong>
</body>
</html>